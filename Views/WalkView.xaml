             x:Class="Walkie_Doggie.Views.WalkView"
             xmlns:pack="clr-namespace:Walkie_Doggie.PacksKit"
             xmlns:vm="clr-namespace:Walkie_Doggie.ViewModels"
             x:DataType="vm:WalkViewModel"
             FlowDirection="RightToLeft">
